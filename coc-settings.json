{
    "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": ["go.mod"],
      "disableWorkspaceFolders": true,
      "filetypes": ["go"]
      }
    },
    "list.source.files.excludePatterns": ["**/.git/**"],
    "diagnostic.errorSign": "✗",
    "diagnostic.warningSign": "⚠︎",
    "suggest.completionItemKindLabels": {
        "keyword": "",
        "variable": "",
        "value": "",
        "operator": "Ψ",
        "function": "⨕",
        "reference": "渚",
        "constant": "",
        "method": "",
        "struct": "פּ",
        "class": "",
        "interface": "",
        "text": "♣",
        "enum": "",
        "enumMember": "",
        "module": "",
        "color": "",
        "property": "",
        "field": "",
        "unit": "",
        "event": "鬒",
        "file": "",
        "folder": "",
        "snippet": "",
        "typeParameter": "", 
        "default": ""
    },
    "coc.preferences.formatOnSaveFiletypes": ["go"],
    "coc.preferences.formatOnType": true,
    "explorer.width": 35,
    "explorer.icon.enableNerdfont": true,
    "explorer.file.tabCD": false,
    "explorer.keyMappings": {
        "i": "true",
        "o": ["expanded?", "collapse", "expand"],
        "cr": ["expanded?", "collapse", "expand"]
    },
    "bookmark.sign": ""
}
